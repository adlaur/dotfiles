# Sanity is for the weak
profile single {
output eDP-1 enable
}

profile {
output eDP-1 disable
output DP-1 enable
exec swaymsg workspace 1, move workspace to output 'DP-1'
exec swaymsg workspace 2, move workspace to output 'DP-1'
exec swaymsg workspace 3, move workspace to output 'DP-1'
exec swaymsg workspace 4, move workspace to output 'DP-1'
exec swaymsg workspace 5, move workspace to output 'DP-1'
exec pkill waybar; waybar &
}

